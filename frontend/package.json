    "@types/react-select": "^3.0.10",
    "@types/webpack": "^4.41.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "bootstrap": "^4.3.1",
    "customize-cra": "^0.9.1",
    "istanbul-lib-coverage": "^2.0.5",
    "jquery": "^3.4.1",
    "material-table": "^1.53.0",
    "nyc": "^14.1.1",
    "papaparse": "^5.1.0",
    "react": "^16.11.0",
    "react-app-rewired": "^2.1.5",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.11.0",
    "react-dropzone": "^10.1.10",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.8",
    "react-spinners": "^0.8.0",
    "typescript": "3.6.4",
    "wait-on": "^4.0.0"
  },
  "scripts": {
    "start": "REACT_APP_STAGE=dev react-app-rewired start",
    "build": "REACT_APP_STAGE=dev react-app-rewired build",
    "build-staging": "react-app-rewired build",
    "test": "cypress run",
    "start-with-coverage": "CYPRESS_COVERAGE=true npm start & wait-on http://localhost:3000/",
    "eject": "react-scripts eject"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 0,
    "lines": 0,
    "functions": 0,
    "statements": 0,
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".tsx",
      ".ts"
    ],
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "exclude": [
      "cypress/"
    ],
    "all": true
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "cypress": "^3.6.1",
    "cypress-file-upload": "^3.5.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1"
  }
